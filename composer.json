{
  "name": "tendopay/laravel-dragonpay-api",
  "description": "DragonPay API Integration",
  "type": "library",
  "authors": [
    {
      "name": "Robert Kłodziński",
      "email": "robert.klodzinski@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7",
    "laravel/framework": "5.5.x|5.6.x|5.7.x",
