{
  "name": "mmichalak87/laravel-dragonpay-api",
  "description": "DragonPay API Integration",
  "type": "library",
  "authors": [
    {
      "name": "Robert Kłodziński",
      "email": "robert.klodzinski@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7",
    "laravel/framework": "5.x|6.x|7.x",
    "phpro/soap-client": "^0.7.11"
  },
  "autoload": {
    "psr-4": {
      "TendoPay\\Integration\\DragonPay\\": "src/TendoPay/Integration/DragonPay",
      "TendoPay\\Integration\\DragonPay\\SoapClient\\": "autogenerated/TendoPay/Integration/DragonPay/SoapClient"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TendoPay\\Integration\\DragonPay\\DragonPayServiceProvider"
      ]
    }
  }
}
